            <value>7441119632</value>
            <value>6268663521</value>
            <null/>
         </list>
      </property>
      <property name="address">
        <set>
            <value>Amla</value>
            <value>Vikrampur</value>
            <value>Manora</value>
            <value>Ganora</value>
        </set>
      </property>
      <property name="course">
